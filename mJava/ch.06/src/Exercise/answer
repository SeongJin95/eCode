11.	(1)Math.random()*156+100 //100~256
	(2)import java.util.Random;
	   Random r = new Random();
	   for(int i=0;i<10;i++){
	   	System.out.print(r.nextInt(156)+100);
	   	System.out.print(" ");
	   }
	   
12.	new Calendar()
	clear()
	set(Calendar.YEAR, 2020)
	set(Calendar.MONTH, 11)
	set(Calendar.DAY_OF_MONTH, 25)
	get(Calendar.YEAR), get(Calendar.MONTH), get(Calendar.DAY_OF_MONTH)+1    	